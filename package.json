{
  "name": "cryptotitans-home",
  "version": "1.0.0",
  "description": "cryptotitans-home",
  "author": "Hydra",
  "license": "MIT",
  "engines": {
    "node": "12||14||16||18||20",
    "npm": "6||8||10"
  },
  "main": "./server/index.js",
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:client": "cross-env BABEL_ENV=production NODE_ENV=production UV_THREADPOOL_SIZE=12 node --max-old-space-size=2048 node_modules/webpack/bin/webpack.js --config webpack.prod.babel.js --mode=production --color --progress",
    "build:server": "cross-env BABEL_ENV=production NODE_ENV=production SSR=1 UV_THREADPOOL_SIZE=12 node --max-old-space-size=2048 node_modules/webpack/bin/webpack.js --mode=production --color --progress",
    "start": "webpack --mode=development --watch",
    "start:ssr": "cross-env NODE_ENV=development SSR=1 webpack --mode=development --watch",
    "pretest": "npm run test:clean",
    "prod": "pm2 startOrReload ecosystem.config.js --env production --update-env",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test node ./node_modules/jest-cli/bin/jest --coverage --coverageDirectory ./coverage --maxWorkers=50%",
    "test:watch": "cross-env NODE_ENV=test node ./node_modules/jest-cli/bin/jest --watch --maxWorkers=25%"
  },
